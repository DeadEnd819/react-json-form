{
  "name": "@undermuz/react-json-form",
  "homepage": "http://undermuz.github.io/react-json-form",
  "version": "1.1.16",
  "description": "Generate JSON-based forms with react",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "themes/*": [
        "dist/themes/*/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "esmodules": "./dist/index.mjs",
      "node": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./themes/chakra": {
      "types": "./dist/themes/chakra/index.d.ts",
      "import": "./dist/themes/chakra/index.mjs",
      "module": "./dist/themes/chakra/index.mjs",
      "esmodules": "./dist/themes/chakra/index.mjs",
      "node": "./dist/themes/chakra/index.js",
      "require": "./dist/themes/chakra/index.js",
      "default": "./dist/themes/chakra/index.js"
    },
    "./themes/grommet": {
      "types": "./dist/themes/grommet/index.d.ts",
      "import": "./dist/themes/grommet/index.mjs",
      "module": "./dist/themes/grommet/index.mjs",
      "esmodules": "./dist/themes/grommet/index.mjs",
      "node": "./dist/themes/grommet/index.js",
      "require": "./dist/themes/grommet/index.js",
      "default": "./dist/themes/grommet/index.js"
    },
    "./themes/rsuite": {
      "types": "./dist/themes/rsuite/index.d.ts",
      "import": "./dist/themes/rsuite/index.mjs",
      "module": "./dist/themes/rsuite/index.mjs",
      "esmodules": "./dist/themes/rsuite/index.mjs",
      "node": "./dist/themes/rsuite/index.js",
      "require": "./dist/themes/rsuite/index.js",
      "default": "./dist/themes/rsuite/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint ./src && tsc --noEmit",
    "build": "npm run build:tsup",
    "build:tsup": "tsup",
    "predeploy": "npm run lint && npm run test && npm run build",
    "prepublishOnly": "npm run predeploy",
    "storybook": "npm run storybook:start",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:deploy": "gh-pages -d storybook-static",
    "storybook:publish": "npm run storybook:build && npm run storybook:deploy"
  },
  "author": {
    "name": "undermuz",
    "email": "undermuz@gmail.com",
    "url": "https://github.com/undermuz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/undermuz/react-json-form.git"
  },
  "license": "MIT",
