{
  "name": "@undermuz/react-json-form",
  "version": "1.0.1",
  "description": "Generate JSON-based form with react",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/types/index.d.js",
  "typings": "build/types/index.d.js",
  "sideEffects": false,
  "files": [
    "build/cjs/",
    "build/esm/",
    "build/types/"
